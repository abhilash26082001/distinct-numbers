#include <stdio.h>
int main(){
int N,i,p,k,count=1,m=0;
  float a[i];
  scanf("%d",&N);
  scanf("%f",&a[0]);
  for(i=1;i<N;i++)
  {
  scanf("%f",&a[i]);
   count=count+1; 
    for(k=0;k<i;k++){
      
      if(a[i]==a[k]){
        m=1;
      }
      count=count-m;
      m=0;
    }
       
  }
  printf("%d",count);
} 
